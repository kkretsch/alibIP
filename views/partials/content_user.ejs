<!-- Example row of columns -->

    <main class="container">
    	<h1>Your current IP history</h1>

    	<table id="iplogdata" style="margin-top: 5em" class="table table-hover" data-pagelen="10" data-pagenum="0">
    		<thead>
    			<tr>
    				<th>TS</th><th>IPV4</th><th>IPV6</th>
    			</tr>
    		</thead>
    		<tbody>
<% entries.forEach(function(entry){ %>
				<tr>
					<td><%= entry.ts %></td>
					<td><%= entry.ipv4 %></td>
					<td><%= entry.ipv6 %></td>
				</tr>
<% }) %>
			</tbody>
		</table>
		<nav aria-label="...">
  			<ul class="pager">
			    <li><a href="#" id="btnprev">Previous</a></li>
			    <li><a href="#" id="btnnext">Next</a></li>
			</ul>
		</nav>

	</main>
