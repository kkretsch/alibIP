BEGIN:VCALENDAR
VERSION:2.0
PRODID:https://alibip.de/lookup/
METHOD:PUBLISH<% entries.forEach(function(entry) { %>
BEGIN:VEVENT
DTSTAMP:<%= entry.icalts %>
DTSTART:<%= entry.icalts %>
<% if(entry.countrefresh > 0) { %>DTEND:<%= entry.icaltsrefresh %><% } %>
UID:<%= entry.id %>@alibip.de
DESCRIPTION:IPv4: <%= entry.ipv4 %>,
 IPv6: <%= entry.ipv6 %>
END:VEVENT<% }); %>
END:VCALENDAR